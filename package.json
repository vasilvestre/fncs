{
  "name": "fncs",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://vasilvestre.github.io/fncs/build",
  "dependencies": {
    "@material-ui/core": "^4.9.0",
    "@material-ui/icons": "^4.5.1",
    "@material/top-app-bar": "^4.0.0",
    "@testing-library/jest-dom": "^5.0.2",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.149",
    "@types/material-ui": "^0.21.7",
    "@types/material__top-app-bar": "^0.35.1",
    "@types/materialize-css": "^1.0.7",
    "@types/md5": "^2.1.33",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.3",
    "email-validator": "^2.0.4",
    "lodash": "^4.17.15",
    "materialize-css": "^1.0.0",
    "md5": "^2.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "typeface-roboto": "0.0.75",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-ci": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "e2e": "cypress run --headless",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/prop-types": "^15.7.3",
    "cypress": "3.8.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-cypress": "^2.8.1",
    "eslint-plugin-prettier": "^3.1.2",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.1",
    "jest": "^24.9.0",
    "prettier": "1.19.1"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 1,
        "functions": 1,
        "lines": 1,
        "statements": -60
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test && npm run e2e"
    }
  },
  "lint-staged": {
    "*.{tsx,js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
