{
  "plugins": ["prettier"],
  "rules": {
    "strict": "off",
    "prettier/prettier": "error"
  },
  "extends": [
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
    "impliedStrict": false
  }
}
